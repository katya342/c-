<Window x:Class="SenderClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SenderClient"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="SenderClient" Height="205" Width="200">
    <StackPanel>
        <TextBlock Height="137" Margin="0,0, -0.4,0" Padding="10" TextAlignment="Center">
            Уведомление
            <LineBreak/>
            <LineBreak/>
            Приложение по приему
            <LineBreak/>
            уведомлений
        </TextBlock>
        <Button x:Name="StartListeningButton" Height="30" Margin="40,0,40,10" Click="StartListeningButton_Click">
            OK
        </Button>
    </StackPanel>
</Window>
